<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.guofengwu.flatbufferdemo.MainActivity"
    android:orientation="vertical"
    >

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FlatBuffers序列化测试"
        android:id="@+id/flatTest" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Flat反序列化测试"
        android:id="@+id/flatDTest"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JAVA序列化测试"
        android:id="@+id/javaTest" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JAVA反序列化测试"
        android:id="@+id/javaDTest"/>



</LinearLayout>
